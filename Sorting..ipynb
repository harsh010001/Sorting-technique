   "source": [
    "def pairwise_swap(array):\n",
    "    n=len(arr)\n",
    "    for i in range(0,n-1):\n",
    "        for j in range(0,n-i-1):\n",
    "            if arr[j]>arr[j+1]:\n",
    "                arr[j],arr[j+1]=arr[j+1],arr[j]\n",
    "                \n",
    "arr=array('i',[64,34,25,12,10,9,8,7,6,5,4,3,2])\n",
    "pairwise_swap(arr)\n",
    "print(arr)"
   ]
  }
